{
  "name": "pi-lib",
  "version": "0.1.0",
  "description": "Monorepo of React UI components and supporting utilities.",
  "scripts": {
    "sb": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "sandbox": "pnpm --filter sandbox dev",
    "build": "npx nx run-many --target=build",
    "change": "pnpm i && beachball sync && pnpm build && beachball change",
    "publish": "npx beachball publish",
    "lint": "npx prettier -w . && npx eslint --fix",
    "test": "npx nx run-many --target=test",
    "generate": "node ./bin/generate.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lancerael/pi.git"
  },
  "keywords": [],
  "author": "Lance Taylor",
  "license": "ISC",
  "beachball": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack4": "^6.5.10",
    "@storybook/manager-webpack4": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "0.0.13",
    "@types/node": "^18.7.11",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "babel-loader": "^8.2.5",
    "beachball": "^2.31.4",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs-extra": "^10.1.0",
    "nx": "^15.0.0",
    "prettier": "^2.8.0"
  },
  "pnpm": {
    "overrides": {
      "trim@<0.0.3": ">=0.0.3",
      "glob-parent@<5.1.2": ">=5.1.2",
      "loader-utils@>=2.0.0 <2.0.4": ">=2.0.4",
      "loader-utils@>=1.0.0 <1.4.2": ">=1.4.2",
      "decode-uri-component@<0.2.1": ">=0.2.1",
      "trim-newlines@<3.0.1": ">=3.0.1",
      "loader-utils@<1.4.1": ">=1.4.1",
      "loader-utils@>=2.0.0 <2.0.3": ">=2.0.3",
      "json5@<1.0.2": ">=1.0.2",
      "json5@>=2.0.0 <2.2.2": ">=2.2.2"
    }
  }
}
