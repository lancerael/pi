{
  "name": "pi-lib",
  "version": "0.1.0",
  "description": "Monorepo of React UI components and supporting utilities.",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/lancerael/pi.git"
  },
  "license": "ISC",
  "author": "Lance Taylor",
  "scripts": {
    "bb:change": "pnpm i && beachball sync && beachball change",
    "bb:publish": "npx beachball publish",
    "build": "npx nx run-many --target=build",
    "demo": "pnpm --filter @pi-lib/tech-demo dev",
    "demo:candlestick:start": "pnpm --filter @pi-lib/candlestick-chart-mfe start",
    "demo:mfes": "concurrently \"pnpm sandbox:start\" \"pnpm candlestick:start\"",
    "demo:sandbox": "pnpm --filter @pi-lib/chart-sandbox-mfe dev",
    "demo:sandbox:api-deploy": "pnpm --filter @pi-lib/chart-sandbox-api release",
    "demo:sandbox:start": "pnpm --filter @pi-lib/chart-sandbox-mfe start",
    "docs": "pnpm docs:readmeGen && pnpm docs:styleDocs && pnpm lint:fix",
    "docs:readmeGen": "node ./bin/readmeGen.ts",
    "docs:styleDocs": "node ./bin/styleDocGen.ts",
    "generate": "node ./bin/generate.ts",
    "lint": "npx prettier -c . && npx eslint",
    "lint:fix": "npx prettier -w . && npx eslint --fix",
    "sb": "pnpm docs && cross-env NODE_OPTIONS=--openssl-legacy-provider storybook dev -p 6006",
    "sb:build": "cross-env NODE_OPTIONS=--openssl-legacy-provider storybook build",
    "test": "npx nx run-many --target=test"
  },
  "devDependencies": {
    "@pi-lib/button": "*",
    "@pi-lib/charts": "*",
    "@pi-lib/config": "*",
    "@pi-lib/link": "*",
    "@pi-lib/loader": "*",
    "@pi-lib/modal-screen": "*",
    "@pi-lib/styles": "*",
    "@pi-lib/tooltip": "*",
    "@pi-lib/utils": "*",
    "@storybook/addon-actions": "^7.0.21",
    "@storybook/addon-backgrounds": "^7.0.21",
    "@storybook/addon-docs": "^7.0.21",
    "@storybook/addon-essentials": "^7.0.21",
    "@storybook/addon-interactions": "^7.0.21",
    "@storybook/addon-links": "^7.0.21",
    "@storybook/addon-mdx-gfm": "7.0.21",
    "@storybook/addon-measure": "^7.0.21",
    "@storybook/addon-outline": "^7.0.21",
    "@storybook/blocks": "^7.0.21",
    "@storybook/client-api": "^7.0.21",
    "@storybook/client-logger": "^7.0.21",
    "@storybook/react": "^7.0.21",
    "@storybook/react-vite": "7.0.21",
    "@storybook/testing-library": "0.1.0",
    "@types/d3-axis": "^3.0.2",
    "@types/d3-fetch": "^3.0.2",
    "@types/d3-force": "^3.0.4",
    "@types/d3-scale": "^4.0.3",
    "@types/d3-shape": "^3.1.1",
    "@types/jest": "^29.4.4",
    "@types/jest-axe": "^3.5.5",
    "@types/lodash.throttle": "^4.1.7",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-redux": "^7.1.25",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react": "^3.1.0",
    "beachball": "^2.31.12",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "d-theia": "^3.6.0",
    "d3-axis": "^3.0.0",
    "d3-geo": "^3.1.0",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-transition": "^3.0.1",
    "esbuild": "^0.17.18",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs-extra": "^11.1.0",
    "jest-mock": "^29.5.0",
    "lodash.throttle": "^4.1.1",
    "nx": "^15.8.6",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-aria": "^3.25.0",
    "react-aria-components": "1.0.0-alpha.4",
    "react-dom": "^18.2.0",
    "react-stately": "^3.23.0",
    "react-svg": "^16.1.15",
    "resize-observer-polyfill": "^1.5.1",
    "storybook": "7.0.21",
    "styled-components": "^5.3.11",
    "typescript": "^4.9.5"
  },
  "beachball": {
    "access": "public"
  },
  "pnpm": {
    "overrides": {
      "trim@<0.0.3": ">=0.0.3",
      "glob-parent@<5.1.2": ">=5.1.2",
      "trim-newlines@<3.0.1": ">=3.0.1"
    }
  }
}
