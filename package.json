{
  "name": "pi-lib",
  "version": "0.1.0",
  "description": "Monorepo of React UI components and supporting utilities.",
  "scripts": {
    "sandbox": "pnpm --filter sandbox dev",
    "build": "npx nx affected --target=build",
    "change": "pnpm i && beachball sync && pnpm build && beachball change",
    "publish": "npx beachball publish",
    "lint": "npx prettier -w . && npx eslint --fix",
    "test": "npx nx affected --target=test",
    "generate": "node ./bin/generate.js",
    "sb": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lancerael/pi.git"
  },
  "keywords": [],
  "author": "Lance Taylor",
  "license": "ISC",
  "beachball": {
    "access": "public"
  },
  "devDependencies": {
    "@pi-lib/button": "^1.0.3",
    "@pi-lib/charts": "^0.8.1",
    "@pi-lib/config": "^0.2.6",
    "@pi-lib/constants": "^0.1.2",
    "@pi-lib/loader": "^2.0.3",
    "@pi-lib/styles": "^1.0.3",
    "@pi-lib/tooltip": "^1.0.3",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-vite": "^0.4.2",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "0.0.13",
    "@types/d3-axis": "^3.0.2",
    "@types/d3-scale": "^4.0.3",
    "@types/jest": "^29.4.4",
    "@types/jest-axe": "^3.5.5",
    "@types/lodash.throttle": "^4.1.7",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react": "^3.1.0",
    "beachball": "^2.31.12",
    "d3-axis": "^3.0.0",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-transition": "^3.0.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs-extra": "^11.1.0",
    "lodash.throttle": "^4.1.1",
    "nx": "^15.8.6",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resize-observer-polyfill": "^1.5.1",
    "styled-components": "^5.3.9",
    "typescript": "^4.9.5"
  },
  "pnpm": {
    "overrides": {
      "trim@<0.0.3": ">=0.0.3",
      "glob-parent@<5.1.2": ">=5.1.2",
      "trim-newlines@<3.0.1": ">=3.0.1"
    }
  }
}
