{
  "name": "pi-lib",
  "version": "0.1.0",
  "description": "Monorepo of React UI components and supporting utilities.",
  "scripts": {
    "sb": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "sandbox": "pnpm --filter sandbox dev",
    "build": "npx nx affected --target=build",
    "change": "pnpm i && beachball sync && pnpm build && beachball change",
    "publish": "npx beachball publish",
    "lint": "npx prettier -w . && npx eslint --fix",
    "test": "npx nx affected --target=test",
    "generate": "node ./bin/generate.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lancerael/pi.git"
  },
  "keywords": [],
  "author": "Lance Taylor",
  "license": "ISC",
  "beachball": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack4": "^6.5.10",
    "@storybook/manager-webpack4": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "0.0.13",
    "@types/jest": "^28.1.8",
    "@types/jest-axe": "^3.5.4",
    "@types/node": "^18.7.11",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@types/lodash.throttle": "^4.1.7",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "babel-loader": "^8.2.5",
    "beachball": "^2.31.4",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs-extra": "^10.1.0",
    "nx": "^15.0.0",
    "prettier": "^2.8.0"
  }
}
