{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "test:coverage"
        ]
      }
    }
  },
  "namedInputs": {
    "dist": [
      "!{projectRoot}/**/dist/**"
    ]
  },
  "targetDefaults": {
    "build": {
      "inputs": [
        "dist",
        "^dist"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "default",
        "^default"
      ]
    },
    "test:coverage": {
      "dependsOn": [
        "^build"
      ]
    }
  }
}
