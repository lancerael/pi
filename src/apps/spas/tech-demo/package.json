{
  "name": "@pi-lib/tech-demo",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "build:local": "tsc && cross-env NODE_ENV=development vite build",
    "dev": "vite --host",
