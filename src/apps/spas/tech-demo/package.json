    "@pi-lib/icon-button": "workspace:^",
    "@pi-lib/input": "workspace:^",
    "@pi-lib/link": "workspace:^",
    "@pi-lib/modal": "workspace:^",
    "@pi-lib/page-loader": "workspace:^",
    "@pi-lib/select": "workspace:^",
    "@pi-lib/styles": "workspace:^",
    "@pi-lib/table": "workspace:^",
    "@pi-lib/toast": "workspace:^",
    "@pi-lib/utils": "workspace:^",
    "@reduxjs/toolkit": "^1.9.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^7.1.1",
